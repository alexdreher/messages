%h1 Listing admin_users

%table
  %tr
    %th Email
    %th Registered at
    %th Sign in count
    %th Current sign in at
    %th
    %th
    %th

  - @admin_users.each do |admin_user|
    %tr
      %td= admin_user.email
      %td= admin_user.created_at
      %td= admin_user.sign_in_count
      %td= admin_user.current_sign_in_at
      %td= link_to 'Show', admin_user_path(admin_user)
      %td= link_to 'Edit', edit_admin_user_path(admin_user)
      %td= link_to 'Destroy', admin_user_path(admin_user), :confirm => 'Are you sure?', :method => :delete

%br

= link_to 'New User', new_admin_user_path
